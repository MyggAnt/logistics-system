{
  "name": "logistics-backend",
  "version": "1.0.0",
  "description": "Backend for logistics management system",
  "main": "dist/server.js",
  "scripts": {
    "build": "tsc",
    "start": "node dist/server.js",
    "dev": "ts-node src/server.ts",
    "watch": "nodemon src/server.ts",
    "seed": "ts-node src/scripts/seed.ts",
    "migrate": "typeorm migration:run",
    "migrate:generate": "typeorm migration:generate",
    "migrate:revert": "typeorm migration:revert",
    "schema:sync": "typeorm schema:sync",
    "schema:drop": "typeorm schema:drop"
  },
  "dependencies": {
    "@apollo/server": "^4.10.1",
    "cors": "^2.8.5",
    "express": "^4.18.2",
    "graphql": "^16.11.0",
    "graphql-subscriptions": "^2.0.0",
    "pg": "^8.16.3",
    "reflect-metadata": "^0.2.2",
    "socket.io": "^4.8.1",
    "type-graphql": "^2.0.0-rc.2",
    "typeorm": "^0.3.25"
  },
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/node": "^20.19.9",
    "@types/pg": "^8.11.0",
    "nodemon": "^3.0.3",
    "ts-node": "^10.9.2",
    "typescript": "^5"
  },
  "keywords": [
    "logistics",
    "express",
    "graphql",
    "typeorm",
    "postgresql",
    "socket.io"
  ],
  "author": "Logistics Team",
  "license": "MIT"
}
